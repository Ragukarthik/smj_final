{
  "expo": {
    "name": "SMJ",
    "slug": "smj",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.ragukarthik.smj_final"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "be8adcf1-0d04-48b8-9500-437a637cc249"
      }
    }
  }
}
